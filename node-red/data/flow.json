    {
        "id": "function_detect_material",
        "type": "function",
        "z": "tab_main",
        "name": "Prepare Detection Event",
        "func": "// Simulate material detection\nconst bin_id = env.get('BIN_ID') || 'test-bin-uuid-001';\nconst user_qr = env.get('USER_QR') || 'SB-test-user-001';\nconst material = msg.material || 'other';\n\n// Confidence between 0.7 and 0.99\nconst confidence = (Math.random() * 0.29 + 0.7).toFixed(2);\n\nmsg.topic = `bin/${bin_id}/detected`;\nmsg.payload = {\n    bin_id: bin_id,\n    user_qr_code: user_qr,\n    material_type: material,\n    confidence: parseFloat(confidence),\n    timestamp: new Date().toISOString(),\n    event: 'material_detected'\n};\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 380,
        "y": 380,
        "wires": [
            [
                "mqtt_publish_detect",
                "debug_detect"
            ]
        ]
    },
