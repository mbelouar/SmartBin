        "id": "function_open_bin",
        "type": "function",
        "z": "tab_main",
        "name": "Prepare Open Command",
        "func": "// Simulate bin open command\nconst bin_id = env.get('BIN_ID') || 'test-bin-uuid-001';\nconst user_qr = env.get('USER_QR') || 'SB-test-user-001';\n\nmsg.topic = `bin/${bin_id}/open`;\nmsg.payload = {\n    bin_id: bin_id,\n    command: 'open',\n    user_qr_code: user_qr,\n    timestamp: new Date().toISOString()\n};\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
